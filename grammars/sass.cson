        }
        {
          'include': '#string-single'
        }
        {
          'include': '#string-double'
        }
      ]
      'beginCaptures':
        '1':
          'name': 'punctuation.embedded.interpolation.sass'
      'endCaptures':
        '1':
          'name': 'punctuation.embedded.interpolation.sass'

    }

  'tags':
    {
