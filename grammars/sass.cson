      'name': 'support.type.property-name.css.sass'
    }
  'property-value':
    'begin': '(:)?\\s?+'
    'beginCaptures':
      '1':
        'name': 'invalid.illegal.punctuation.sass'
    'end': '(;)?$'
    'endCaptures':
      '1':
        'name': 'invalid.illegal.punctuation.sass'
    'name': 'meta.property-value.sass'
    'patterns': [
      {
        'match': '([-=+*/])'
        'name': 'punctuation.definition.entity.css'
      }
      {
        'include': '#interpolations'
      }
      {
        'captures':
          '1':
            'name': 'punctuation.definition.entity.css'
          '2':
            'name': 'variable.other.sass'
