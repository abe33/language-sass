      '1':
        'name': 'meta.variable.sass'
      '2':
        'name': 'punctuation.definition.entity.css'
      '3':
        'name': 'punctuation'
    'end': '$'
    'name': 'meta.set.variable.sass'
    'patterns': [
      {
        'include': '#property-value'
      }
      {
        'include': '#variables'
      }
    ]

